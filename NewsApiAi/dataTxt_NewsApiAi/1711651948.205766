a"}}}, "categories": [{"uri": "dmoz/Sports/Basketball/Professional", "label": "dmoz/Sports/Basketball/Professional", "wgt": 88}, {"uri": "dmoz/Sports/Basketball/Fan_Pages", "label": "dmoz/Sports/Basketball/Fan Pages", "wgt": 77}, {"uri": "news/Sports", "label": "news/Sports", "wgt": 91}], "articleCounts": {"eng": 86, "spa": 34, "deu": 11}, "sentiment": 0.01960784313725483, "wgt": 449020800, "relevance": 8}